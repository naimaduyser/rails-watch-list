<h1>Add A Movie:</h1>
<%= form_for(@movie) do |f| %>
  <%= f.label :title %>
  <br>
  <%= f.text_field :title %>
  <br>
  <br>
  <%= f.label :year %>
  <br>
  <%= f.text_field :year %>
  <br>
  <br>
  <%= f.label :director %>
  <br>
  <%= f.text_field :director %>
  <br>
  <br>
  <%= f.label :genre %>
  <br>
  <%= f.text_field :genre %>
  <br>
  <br>
  <%= f.label :synopsis %>
  <br>
  <%= f.text_area :synopsis%>
  <br>
  <br>
  <%= f.submit %>
<% end %>
